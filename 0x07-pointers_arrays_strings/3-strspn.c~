#include "holberton.h"





unsigned int _strspn(char *s, char *accept)
{
  int i;
  int j = 0;
  int n = 0;

  for (i = 0; s[i] != '\0'; i++)
    {
      while (accept[j] != '\0')
	{
	  if (accept[j] == s[i])
	  {
	    n = 1;
	  }
      if (n == 0)
	{
	  return (i);
	  }
      j++;
	}
      return (i);
    }
}
