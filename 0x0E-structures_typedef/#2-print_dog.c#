#include "dog.h"
#include <stdio.h>



void print_dog(struct dog *d)
{
  if(d != 0)
    {
      if (d->name != NULL)
	{
	  printf("Name: %s\n", d->name);
	}
      else
	{
	  printf("Name: (nil)\n");
	}
      if (d->age != NULL)
	{
	  printf("Age: %s\n", d->age);
	}
      else
	{
	  printf("Age: (nil)\n");
	}
